        "guzzlehttp/guzzle": "^6.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/flex": "^1.3.1",
        "symfony/messenger": "4.4.*",
        "symfony/yaml": "4.4.*",
        "ramsey/uuid": "^3.8",
        "doctrine/dbal": "^2.9",
        "doctrine/orm": "^2.6",
        "ocramius/proxy-manager": "^2.6",
        "monolog/monolog": "^2.0",
        "tedivm/stash": "^0.14",
        "phpspec/phpspec": "^5.1",
